package zombiegame;

import java.awt.Color;

public class Building {

	double x;
	double y;
	double width;
	double height;
	Color colour;
	
	Building (double x, double y, double width, double height, Color colour) {
		this.x = x;
		this.y = y;
		this.width = width;
		this.height = height;
		this.colour = colour;
	}
	
}
