package zombiegame;

public class Bullet {

	int width;
	int height;
	int posX;
	int posY;
	int speedX;
	int speedY;
	
	Bullet(int width, int height, int posX, int posY, int speedX, int speedY) {
		
		this.width = width;
		this.height = height;
		this.posX = posX;
		this.posY = posY;
		this.speedX = speedX;
		this.speedY = speedY;
	}
	
}
